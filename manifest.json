{
    "manifest_version": 2,
    "name": "Epoch Converter",
    "version": "1.0",
    "description": "Convert selected epoch values to human-readable format.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  